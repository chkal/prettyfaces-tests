<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
  version="1.2"> 

  <application>
    <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  </application>

  <managed-bean>
    <managed-bean-name>welcomeBean</managed-bean-name>
    <managed-bean-class>de.chkal.prettytest.WelcomeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>welcomeBean2</managed-bean-name>
    <managed-bean-class>de.chkal.prettytest.WelcomeBean2</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>greetingBean</managed-bean-name>
    <managed-bean-class>de.chkal.prettytest.GreetingBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

</faces-config>